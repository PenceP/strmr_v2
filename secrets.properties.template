# Copy this file to secrets.properties and fill in your API keys
# Do not commit secrets.properties to version control

# Trakt API Keys
# Get these from https://trakt.tv/oauth/applications/new
TRAKT_CLIENT_ID=your_trakt_client_id_here
TRAKT_CLIENT_SECRET=your_trakt_client_secret_here

# TMDB API Keys
# Get these from https://www.themoviedb.org/settings/api
TMDB_API_KEY=your_tmdb_api_key_here
TMDB_ACCESS_TOKEN=your_tmdb_access_token_here